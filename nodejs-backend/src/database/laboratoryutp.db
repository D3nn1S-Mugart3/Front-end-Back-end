-- Crear la base de datos
CREATE DATABASE laboratoryutp;

-- Usar la base de datos creada
USE laboratoryutp;

-- Tabla computadoras
CREATE TABLE IF NOT EXISTS computadoras (
    id INT AUTO_INCREMENT PRIMARY KEY,
    nombre VARCHAR(255),
    marca VARCHAR(255),
    estado VARCHAR(255),
    numSerie VARCHAR(255),
    teclado VARCHAR(255),
    mouse VARCHAR(255)
);